a)
 i=5;
 j=++i*3-2;
 printf("%d %d", i,j);
output=6 16
b)
 i=5;
 j=3-2*i++;
 printf("%d %d", i,j);
output=6 -7
c)
 i=7;
 j=3*i--+2;
 printf("%d %d", i,j);
output=6 23
d)
 i=7;
 j=3+--i*2;
 printf("%d %d", i,j);
output=6 15

